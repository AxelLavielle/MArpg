            NBkey = NBkey + 1;
        }
        Tour = Tour + 1;//On passe au tour des Troopers
        yP = yP+1;
      }
    }
    
    else if (keyCode == RIGHT) {//Si il relache la fleche de droite
      if (Maze[yP][xP+1] != 1){//On verifie qu'il n'y a pas de murs
        keyCode = 0;
        fill(Maze[yP][xP]*40);//On deplace la princesse vers la droite
        rect(xP*50, yP*50, 50, 50);
        if(Maze[yP][xP] == 3){//Si il y a une cle, la princesse la prend.
            fill(0);
            rect(xP*50, yP*50, 50, 50);
            Maze[yP][xP] = 0;
            NBkey = NBkey + 1;
        }
        Tour = Tour + 1;//On passe au tour des Troopers
        xP = xP+1;
      }
    }

    else if (keyCode == LEFT) {//Si il relache la fleche de gauche.
      if (Maze[yP][xP-1] != 1){//On verifie qu'il n'y a pas de murs
        keyCode = 0;
