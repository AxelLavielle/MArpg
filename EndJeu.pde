class Ground { // Objet ground qui gere les arbres leur deplacement leur fluidité, et les tunnels
  int speed;
  int slowMove= millis();
  int decal= 0;
  int fin = 41;
  
  Ground(int s) {
    speed= 1600/s;
  }
  void update() {
    g.dispTree();
    decal++;
    if (slowMove < millis()-speed) { // Fluidité des arbres
      g.moveTree();
      slowMove= millis();
      decal= 0;
      fin--;
      if (fin == 0) {
        table[0][0]= table[0][1]= table[0][2]= 2;
      }
    }
    image(imgPrincesse, (xPrincesse-1)*100+decalPrincesse+250, 600);
  }
  
  void moveTree() { // Bouger les arbres
    for (int i= table.length-1; i > 0; i--) {// On baisse tous les arbres
      table[i][0]= table[i-1][0];
      table[i][1]= table[i-1][1];
      table[i][2]= table[i-1][2];
    }
    if (table[0][0] == 0 && table[0][1] == 0 && table[0][2] == 0) {//On place deux arbres aléatoirement
      table[0][0]= table[0][1]= table[0][2]= 0;
      randTree();
    }
    else {
      table[0][0]= table[0][1]= table[0][2]= 0;
    }
  }
  
  void randTree() { // Nouvelle position aleatoire
    table[0][int(random(3))]= 1;
    table[0][int(random(3))]= 1;
  }
  
  void dispTree() { // Afficher les arbres
    background(0);
    for (int i= 0; i < table.length; i++) {
      for (int j= 0; j < table[0].length; j++) {
        if (table[i][j] == 1) {
          image(Arbre,j*100+250, i*100+50+decal*(1600/speed));
        }
        else if (table[i][j] == 2) {
          image(Tunnel,j*100+250, i*100+decal*(1600/speed));
        }
      }
    }
  }
  boolean looseTest(int x) { // Fonction qui test la defaite
    if (table[5][x-1] == 1 && decal*(1600/speed) > 50 || table[6][x-1] == 1 && decal*(1600/speed) < 20) {
      fin = 41;
      return true;
    }
    return false;
  }
  
  boolean winTest(int x) { // Fonction qui test le gain
    if (table[5][x-1] == 2 && decal*(1600/speed) > 50 || table[6][x-1] == 2 && decal*(1600/speed) < 20) {
      return true;
    }
    return false;
  }
}


