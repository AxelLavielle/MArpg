class fumerolles{ //Objet petite fumerolle
  int xPos = int(random(80));
  int yPos = 1;
  int yPos2 = yPos - 1;
  int yPos3 = yPos2 - 1;
  int stop = 0;
  fumerolles(){
  }
  void Affichage(){
    if (Compteur < 70){//Le compteur permet de savoir quelle couleur mettre pour les fumerolles (en fonction de leur position x)
      B = int(255-(xPos*(255/70)));
    }
    if (140 > Compteur && Compteur > 69){
      G = int(255-(xPos*(255/70)));
    }
    if (Compteur > 139){
      R = int(255-(xPos*(255/70)));
    }
    if (yPos < 70){//Si que la fumerolle n'est pas en bas de l'ecran
      if (stop == 0){//Si la fumerolle est en mouvement
        if (Matrice[yPos+1][xPos] == 1){//Si la fumerolle arrive sur une case ou elle doit se coller, elle descend et se colle.
          fill(0);
          rect(xPos*10,yPos*10,10,10);
          fill(0);
          rect(xPos*10,yPos2*10,10,10);
          fill(0);
          rect(xPos*10,yPos3*10,10,10);
          yPos3 = yPos2;
          yPos2 = yPos;
          yPos = yPos + 1;
          stop = 1;
          fill(R,G,B,255);
          rect(xPos*10,yPos*10,10,10);
          Matrice[yPos][xPos] = 0;
        }
        if (Matrice[yPos][xPos] == 0){//Si la fumerolle arrive sur une case vide, elle descend.
          fill(0);
          rect(xPos*10,yPos3*10,10,10);
          fill(0);
          rect(xPos*10,yPos2*10,10,10);
          fill(0);
          rect(xPos*10,yPos*10,10,10);
          yPos3 = yPos2;
          yPos2 = yPos;
          yPos = yPos + 1;
          fill(R,G,B,255);
          rect(xPos*10,yPos*10,10,10);
          fill(R,G,B,191);
          rect(xPos*10,yPos2*10,10,10);
          fill(R,G,B,128);
          rect(xPos*10,yPos3*10,10,10);
        }
      }
      if(stop == 1){//Si la fumerolle est a l'arret, on l'initialise
        fill(R,G,B,255);
        rect(xPos*10,yPos*10,10,10);
      }
    }
    if(yPos == 70){//Si la fumerolle est en bas de l'ecran, on la sort de l'ecran.
      fill(0);
      rect(xPos*10,yPos*10,10,10);
      fill(0);
      rect(xPos*10,yPos2*10,10,10);
      fill(0);
      rect(xPos*10,yPos3*10,10,10);
      yPos = yPos + 1;
    }   
  }
}

void setup() {//Initialisation de toute les variables pour le "Race", de la taille de la fenetre et des images
  Arbre = loadImage("Arbre.jpg");
  Princesse = loadImage("Haine.jpg");
  Trooper = loadImage("Trooper.jpg");
  Cadenas = loadImage("Cadenas.jpg");
  Clef = loadImage("Clef.jpg");
  Echelle = loadImage("Echelle.jpg");
  size(800, 700);
  background(#B09069);
  xPrincesse= 2;
  sec= second();
  decalPrincesse= 0;
  imgPrincesse= loadImage("princesse_dos.jpg");
  Tunnel = loadImage("tunnel.jpg");
  g= new Ground(4);
  fill(#119f22);
}

void draw() {
  if (Partie == 0){//Partie du Race:
    if (decalPrincesse > 0) {//Pour fluidifier le decalage de la princesse
      decalPrincesse+= 22;
    }
    else if (decalPrincesse < 0) {
      decalPrincesse-= 22;
    }
    if (decalPrincesse >= 100 || decalPrincesse <= -100) {//La princesse s'arrete si elle s'est decaler d'une case
      xPrincesse+= decalPrincesse/abs(decalPrincesse);
      decalPrincesse= 0;
    }
    
    if (g.looseTest(xPrincesse)) {//Test si on a perdu grace a une fonction
      for(int i = 0; i < 7; i++) {
        for(int j = 0; j < 3; j++){
          table[i][j]= 0;
        }
      }
      background(0);
    }
    if (g.winTest(xPrincesse)) {//Test si on a gagner grace a une fonction
      Partie = Partie + 1;
    }
    g.update();//Update du sol (des arbres, et des tuyaux)
  }
  
  
  if (Partie == 1){//Partie d'initialisation pour le PacMan:
      for(int i = 0; i < 14; i++) {
        for(int j = 0; j < 16; j++){
          fill(Maze[i][j]*40);
          rect(j*50, i*50, 50, 50);
        }
      }
      Partie = Partie + 1;
  }
  
  
  if (Partie == 2){//Partie du PacMan:
    for(int i = 0; i < 14; i++) {//On place les cles
      for(int j = 0; j < 16; j++){
        if(Maze[i][j] == 3){
          image(Clef, j*50, i*50);
        }      
      }
    }
    image(Princesse,xP*50,yP*50);//On place les personnages
    image(Trooper, xT*50,yT*50);
    image(Trooper, xT2*50,yT2*50);
    image(Trooper, xT3*50,yT3*50);
    image(Trooper, xT4*50,yT4*50);
    if(lock == 0){ //Si la porte n'est pas fermee
      if(xP == 5 && yP == 6) {//Si le personnage sors des cases de depart, la porte se referme derriere lui.
        Maze[6][6] = 1;
        image(Cadenas, 300, 300);
        lock = 1;
      }
    }
    if(NBkey == 6){//Si le joueur a reuni les 6 cles, la porte se reouvre et une echelle pour remonter apparait.
      Maze[6][6] = 0;
      image(Echelle, 300,300);
      if(xP == 6 && yP == 6){
        background(0);
        Partie = Partie + 1;
      }
    }
    
    Rand_1 = Rand; //On sauvegarde les randoms
    Rand_2 = Rand2;
    Rand_3 = Rand3;
    Rand_4 = Rand4;
    
    while(Tour == 1){//Quand c'est le tour du Trooper 1
      Rand = int(random(4));//On choisit aleatoirement la direction dans laquelle va aller le Trooper
      while(Rand == 3-Rand_1){//Mais on lui empeche de revenir sur ses pas.
        Rand = int(random(4));
      }
      if (Rand == 0){//Si le Trooper a obtenu un 0, il va vers le bas
        if (Maze[yT+1][xT] !=1){//Sauf si il y a un mur, il cherche alors une nouvelle directon.
          fill(Maze[yT][xT]*40);
          rect(xT*50, yT*50, 50, 50);
            if(Maze[yT][xT] == 3){//Si il y avait une cle sous ses pieds, elle apparait.
              image(Clef, xT*50, yT*50);
            }
          yT = yT+1;
          Tour = 2;//On passe au trooper numero 2
        }
      }
