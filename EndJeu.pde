void keyPressed() {//Lorsque une touche est appuyee  
  if (keyCode == RIGHT && xPrincesse != 3 && decalPrincesse == 0 && !g.looseTest(xPrincesse+1)) { //Si le joueur appui sur la fleche de droite, la princesse va a droite.
    decalPrincesse= 20;
  }
  else if (keyCode == LEFT && xPrincesse != 1 && decalPrincesse == 0 && !g.looseTest(xPrincesse-1)) {//Si le joueur appui sur la fleche de gauche, la princesse va a gauche.
    decalPrincesse= -20;
  }
}

void keyReleased(){//Lorsque une touche est relachee
  if (key == CODED && Partie == 2) {//Lorsque le joueur relache une touche
    if (keyCode == UP) {//Si il relache la fleche du haut.
      if (Maze[yP-1][xP] != 1){//On verifie qu'il n'y a pas de murs
        keyCode = 0;
        fill(Maze[yP][xP]*40);//On deplace la princesse vers le haut
        rect(xP*50, yP*50, 50, 50);
        if(Maze[yP][xP] == 3){//Si il y a une cle, la princesse la prend.
            fill(0);
            rect(xP*50, yP*50, 50, 50);
            Maze[yP][xP] = 0;
            NBkey = NBkey + 1;
          }
        Tour = Tour + 1;//On passe au tour des Troopers
        yP = yP-1;   
      }
    }    

    else if (keyCode == DOWN) {//Si il relache la fleche du bas.
      if (Maze[yP+1][xP] != 1){//On verifie qu'il n'y a pas de murs
        keyCode = 0;
        fill(Maze[yP][xP]*40);//On deplace la princesse vers le bas
        rect(xP*50, yP*50, 50, 50);
